@{
    //Layout = "~/Views/Shared/_Layout.cshtml";
    Layout = null;
    ViewBag.Title = "Index";
    @Scripts.Render("~/bundles/jquery")
}
<div>
    <input type="button" class="btn btn-primary" id="OpenSearch" value="Search" style="margin-top:20px" />
</div>
<div>
    <input type="button" class="btn btn-primary" id="OpenInstantCall" value="InstantCall" style="margin-top:20px" />
</div>
<div class="row" style="height:450px;padding-top:20px;">
    <iframe style="width:100%;height:100%" id="iframe" name="myiframe"></iframe>
</div>
<div>
    <form style="display: none" action='https://localhost:44369/ObounlWeb/Search' target="myiframe" method="POST" id="Searchform">
        <input type="hidden" id="agentID" name="agentID" value="FrO5Suo69KUiCTDJDdcCqw==" /> @*A111111111*@
        <input type="hidden" id="token" name="token" value="eUhMyM5XPkmDizIQp/XTjFZq4iYzxQ2UpnLCA1YwiuM=" />
    </form>
</div>
<div>
    <form style="display: none" action='https://localhost:44369/ObounlWeb/InstantCall' target="myiframe" method="POST" id="InstantCallform">
        <input type="hidden" id="CaseNo" name="CaseNo" value="ljDcx6ZrN9IvZlu2RviNgA==" />  @*1234567890*@
        <input type="hidden" id="token" name="token" value="eUhMyM5XPkmDizIQp/XTjFZq4iYzxQ2UpnLCA1YwiuM=" />
    </form>
</div>
<script type="text/javascript">
    $(function () {
        $('#OpenSearch').on('click', function () { $('#Searchform').submit(); });
        $('#OpenInstantCall').on('click', function () { $('#InstantCallform').submit(); });
    });
</script>



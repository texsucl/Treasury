        //計算單張面額,面額小計及股數小計
        function setDenomination_Total() {
            var _Stock_Total = parseInt($('#' + Stock_Total).val());
            var _Amount_Per_Share = parseInt($('#' + Amount_Per_Share).val());
            var _Single_Number_Of_Shares = parseInt($('#' + Single_Number_Of_Shares).val());
            if (!isNaN(_Stock_Total) && !isNaN(_Amount_Per_Share) && !isNaN(_Single_Number_Of_Shares))//張數,每股金額及單張股數皆有值
            {
                //計算單張面額
                $('#' + Denomination).html(customerUtility.addCommas(_Amount_Per_Share * _Single_Number_Of_Shares));
                //計算面額小計
                $('#' + Denomination_Total).html(customerUtility.addCommas(_Stock_Total * _Amount_Per_Share * _Single_Number_Of_Shares));
                //計算股數小計
                $('#' + Number_Of_Shares).val(_Stock_Total * _Single_Number_Of_Shares);
            }
            else if (!isNaN(_Amount_Per_Share) && !isNaN(_Single_Number_Of_Shares))//每股金額及單張股數有值
            {
                //計算單張面額
                $('#' + Denomination).html(customerUtility.addCommas(_Amount_Per_Share * _Single_Number_Of_Shares));

                $('#' + Denomination_Total).html('');
                $('#' + Number_Of_Shares).val('');
            }
            else if (!isNaN(_Stock_Total) && !isNaN(_Single_Number_Of_Shares))//張數及單張股數
            {
                //計算股數小計
                $('#' + Number_Of_Shares).val(_Stock_Total * _Single_Number_Of_Shares);

                $('#' + Denomination).html('');
                $('#' + Denomination_Total).html('');
            }
            else
            {
                $('#' + Denomination).html('');
                $('#' + Denomination_Total).html('');
                $('#' + Number_Of_Shares).val('');
            }
        }


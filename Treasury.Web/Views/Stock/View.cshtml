                    if (data.vSingle_Number_Of_Shares != "null")
                        $('#' + Single_Number_Of_Shares).val(data.vSingle_Number_Of_Shares);
                    if (data.vDenomination != "null")
                        $('#' + Denomination).html(customerUtility.addCommas(data.vDenomination));
                    if (data.vDenominationTotal != "null")
                        $('#' + Denomination_Total).html(customerUtility.addCommas(data.vDenominationTotal));
                    if (data.vNumberOfShares != "null")
                        $('#' + Number_Of_Shares).val(data.vNumberOfShares);
                    $('#' + Memo).val(data.vMemo);
                }
            }
        }


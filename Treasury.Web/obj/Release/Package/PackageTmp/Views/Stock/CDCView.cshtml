        //取得股票明細內容
        function GetCDCStockDetailDate(listId, rowid) {
            var data = $("#" + listId).getRowData(rowid);
            $.ajax({
                type: "POST",
                data: JSON.stringify({
                    Group_No: data.vIB_Group_No,
                    Trea_Batch_No: data.vTrea_Batch_No,
                    Aply_No: data.vAply_No
                }),
                dataType: "json",
                url: CDC_StockUrl.GetCDCStockDetailDate,
                contentType: 'application/json',
            }).done(function (result) {
                CDC_Stock_D_DbGrid();
            })

        }

        //申請覆核
        function CDC_StockApplyFun() {
            $.ajax({
                type: "POST",
                dataType: "json",
                url: CDC_StockUrl.ApplyDbData,
                contentType: 'application/json',
            }).done(function (result) {
                customerUtility.alertAuto(result);
                if (result.RETURN_FLAG) {
                    customerUtility.closeDialog($('#CDC_StockApply'));
                }
            })
        }

        //修改股票資料
        function CDC_StockUpdateTempFun()
        {
            if ($('#' + CDC_StockFormId).valid()) {
                //區域為國內-必要欄位檢核
                if ($("#lIB_Area").text() == "國內") {
                    switch ($('#' + CDC_StockStock_Type).val()) {
                        case "S"://股票
                            if (Check_S() == false) {
                                return
                            }
                            break;
                        case "P"://股權(持股)憑證
                            if (Check_P() == false) {
                                return
                            }
                            break;
                        case "C"://其他
                            if (Check_C() == false) {
                                return
                            }
                            break;
                    }
                }
                else if ($("#lIB_Area").text() == "國外") {
                    if (!Check_F()) {
                        customerUtility.alert('除了類型欄位有值外,需再有一個欄位有值才算有新增存入明細', 'w');
                        return false;
                    }
                }
                $.ajax({
                    type: "POST",
                    data: JSON.stringify({
                        model: CDC_StockDetailViewModel(
                            $('#' + CDC_StockItemId).val(),
                            $('#' + CDC_StockStock_Type).val(),
                            $('#' + CDC_StockStock_No_Preamble).val(),
                            $('#' + CDC_StockStock_No_B).val(),
                            $('#' + CDC_StockStock_No_E).val(),
                            $('#' + CDC_StockStock_Total).val(),
                            $('#' + CDC_StockAmount_Per_Share).val(),
                            $('#' + CDC_StockSingle_Number_Of_Shares).val(),
                            customerUtility.removeCommas($('#' + CDC_StockDenomination).html()),
                            customerUtility.removeCommas($('#' + CDC_StockDenomination_Total).html()),
                            $('#' + CDC_StockNumber_Of_Shares).val(),
                            $('#' + CDC_StockMemo).val())
                    }),
                    dataType: "json",
                    url: CDC_StockUrl.UpdateDbData,
                    contentType: 'application/json',
                }).done(function (result) {
                    customerUtility.alertAuto(result);
                    if (result.RETURN_FLAG) {
                        customerUtility.closeDialog($('#CDC_StockUpdateTemp'));
                        CDC_Stock_D_DbGrid();
                        GetNumber_Of_Shares_Total_Aft();
                        CDC_StockConfirmFlag = result.Datas;
                    }
                })
            }
        }

        //區域=國外  除了類型欄位有值外,需再有一個欄位有值才算有新增存入明細
        function Check_F() {
            var flag = false;
            $('.CDC_StockUpdateType:not(#dStock_Type)').each(function () {
                if ($(this).val().trim() != '')
                    flag = true;
            })
            return flag;
        }

        //股票必填檢查
        function Check_S() {
            if ($("#tStock_No_Preamble").val().trim() == "" || $("#tStock_No_Preamble").val().trim() == "null") {
                customerUtility.alert('序號前置碼必填', 'w');
                return false;
            }

            if ($("#tStock_No_B").val().trim() == "" || $("#tStock_No_B").val().trim() == "null") {
                customerUtility.alert('序號(起)必填', 'w');
                return false;
            }

            if ($("#tStock_No_E").val().trim() == "" || $("#tStock_No_E").val().trim() == "null") {
                customerUtility.alert('序號(迄)必填', 'w');
                return false;
            }

            var _Check_No_B = parseInt($("#tStock_No_B").val());
            var _Check_No_E = parseInt($("#tStock_No_E").val());
            if (_Check_No_B > _Check_No_E) {
                customerUtility.alert('迄號不可小於起號!', 'w');
                return false;
            }

            if ($("#tStock_Total").val().trim() == "" || $("#tStock_Total").val().trim() == "null") {
                customerUtility.alert('張數不可為空白', 'w');
                return false;
            }

            if ($("#tAmount_Per_Share").val().trim() == "" || $("#tAmount_Per_Share").val().trim() == "null") {
                customerUtility.alert('每股金額必填', 'w');
                return false;
            }
            else if (parseInt($("#tAmount_Per_Share").val().trim()) <= 0) {
                customerUtility.alert('每股金額必須大於等於1', 'w');
                return false;
            }

            if ($("#tSingle_Number_Of_Shares").val().trim() == "" || $("#tSingle_Number_Of_Shares").val().trim() == "null") {
                customerUtility.alert('單張股數必填', 'w');
                return false;
            }

            if ($("#tNumber_Of_Shares").val().trim() == "" || $("#tNumber_Of_Shares").val().trim() == "null") {
                customerUtility.alert('股數小計必填', 'w');
                return false;
            }

            return true;
        }

        //股權(持股)憑證必填檢查
        function Check_P() {
            if ($("#tStock_Total").val().trim() == "" || $("#tStock_Total").val().trim() == "null") {
                customerUtility.alert('張數必填', 'w');
                return false;
            }

            if ($("#tNumber_Of_Shares").val().trim() == "" || $("#tNumber_Of_Shares").val().trim() == "null") {
                customerUtility.alert('股數小計必填', 'w');
                return false;
            }

            if ($("#tSingle_Number_Of_Shares").val().trim() == "" || $("#tSingle_Number_Of_Shares").val().trim() == "null") {
                customerUtility.alert('單張股數必填', 'w');
                return false;
            }

            return true;
        }

        //其他必填檢查
        function Check_C() {
            if ($("#tStock_Total").val().trim() == "" || $("#tStock_Total").val().trim() == "null") {
                customerUtility.alert('張數必填', 'w');
                return false;
            }

            if ($("#tMemo").val().trim() == "" || $("#tMemo").val().trim() == "null") {
                customerUtility.alert('備註必填', 'w');
                return false;
            }

            return true;
        }

        function CDC_StockDetailViewModel(
            vItemId,
            vStockType,
            vStockNoPreamble,
            vStockNoB,
            vStockNoE,
            vStockTotal,
            vAmount_Per_Share,
            vSingle_Number_Of_Shares,
            vDenomination,
            vDenominationTotal,
            vNumberOfShares,
            vMemo
        ) {
            var obj = {};
            obj['vItemId'] = vItemId;
            obj['vStock_Type'] = vStockType;
            obj['vStock_No_Preamble'] = vStockNoPreamble;
            obj['vStock_No_B'] = vStockNoB;
            obj['vStock_No_E'] = vStockNoE;
            obj['vStock_Cnt'] = vStockTotal;
            obj['vAmount_Per_Share'] = vAmount_Per_Share;
            obj['vSingle_Number_Of_Shares'] = vSingle_Number_Of_Shares;
            obj['vDenomination'] = vDenomination;
            obj['vDenominationTotal'] = vDenominationTotal;
            obj['vNumberOfShares'] = vNumberOfShares;
            obj['vMemo'] = vMemo;
            return obj;
        }

    });

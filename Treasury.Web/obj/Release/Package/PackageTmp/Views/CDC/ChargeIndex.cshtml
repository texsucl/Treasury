            function formattervStatus(cellvalue, options, rdata) {               
                switch (rdata.vStatus) {
                        @{
                            foreach (var item in ((List<SelectOption>)ViewBag.Sataus))
                            {
                                <text>
                                case '@item.Value':
                                return '@item.Text';
                                </text>
                            }
                        }
                    default:
                return '';
                }


@{
    ViewBag.Title = "TeleEc";
}

<h2>TeleEc</h2>

<input type="text" value="10" id="txt" />
<input type="button" value="click" id="btn" />

@section scripts{
    <script>
        $(function () {
            $('#btn').click(function () {
                for (var i = 0; i < $('#txt').val(); ++i) {
                    $.ajax({
                        type: 'POST',
                        url: "/Tele/TeleEc",
                        data: ""
                    });
                }
            });
        });
    </script>
}
